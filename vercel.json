{
  "buildCommand": "cd client && npm install && npm run build",
  "outputDirectory": "client/dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://ai-powered-meeting-notes-summarizer-and-aflg.onrender.com/api/$1"
    }
  ],
  "routes": [
    { 
      "handle": "filesystem" 
    },
    { 
      "src": "/(.*)", 
      "dest": "/index.html" 
    }
  ]
} 